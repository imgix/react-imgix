{
  "name": "react-imgix",
  "version": "3.0.0",
  "description": "React Component for displaying an image from Imgix",
  "main": "lib/index.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "standard && mocha --require babel-core/register --recursive src/*.test.js",
    "test:watch": "mocha --require babel-core/register --recursive src/*.test.js --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgix/react-imgix.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Frederick Fogerty",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imgix/react-imgix/issues"
  },
  "homepage": "https://github.com/imgix/react-imgix#readme",
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "dependencies": {
    "jsuri": "^1.3.1",
    "js-base64": "~2.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.3",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "expect": "^1.12.2",
    "expect-jsx": "^2.1.0",
    "mocha": "^2.3.3",
    "mocha-babel": "^3.0.0",
    "react": "^0.14",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.2",
    "skin-deep": "^0.8.1",
    "standard": "^5.3.1",
    "tap": "^2.1.1"
  }
}
